<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>


	<servlet>
		<servlet-name>payelectricityservlet</servlet-name>
		<servlet-class>com.wallet.paybill.servlet.PayElectricityServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>calculateservlet</servlet-name>
		<servlet-class>com.wallet.paybill.servlet.CalclulateElectricityBillServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>payelectricityservlet</servlet-name>
		<url-pattern>/payelectricity</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>calculateservlet</servlet-name>
		<url-pattern>/calculate</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>current_bill.html</welcome-file>
	</welcome-file-list>

</web-app>
